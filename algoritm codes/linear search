package codes;
import java.util.Scanner;
public class linear {
	public static void main(String args[])
	{
		Scanner scan = new Scanner (System.in);
		int i;
		int j = scan.nextInt();
		int[] a = new int[j];
		for( i=0;i<j;i++)
		{
			 a[i]=scan.nextInt();
			
		}
		int n = scan.nextInt();
		for(i=0;i<j-1;i++)
		{
			if(a[i]==n)
			{
				System.out.println("number is located at:"+i);
			    
			}
			else
				System.out.println("failed");
		}
		
		scan.close();
	}

}
